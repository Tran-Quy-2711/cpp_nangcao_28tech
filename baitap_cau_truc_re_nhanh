#23 
  int n,m; cin>>n>>m;
  int Min=0,Max=n;
  if(n%2==0) Min=n/2;
  else Min=n/2+1;
  int toithieu=(Min+m-1)/m*m;
  if(toithieu<=Max) cout<<toithieu;
  else cout<<-1;
