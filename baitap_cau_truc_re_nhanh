  int n,a,b; cin>>n>>a>>b;
  if(2*a<=b) cout<<n*a;
  else{
    if(n%2==0) cout<<n/2*b;
    else cout<<n/2*b+a;            //n/2*b+n%2*a ; (n-1/2)*b+a 
  }
