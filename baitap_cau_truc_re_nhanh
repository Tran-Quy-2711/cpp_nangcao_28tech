  int n,m,a; cin>>n>>m>>a;
  int doc,ngang;
  if(n%a==0) doc=n/a;
  else doc=n/a+1;
  if(m%a==0) ngang=m/a;
  else ngang=m/a+1;
  cout<<ngang*doc;
