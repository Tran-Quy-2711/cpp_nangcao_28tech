  char c; cin>>c;
  if(c=='z'||c=='Z') cout<<"a"; 
  else if(c>=65&&c<=90) cout<<char(c+33);
  else if(c>=97&&c<=122) cout<<char(c+1);
  else cout<<"INVALID";
#17
  char c; cin>>c;
  if(c>=65&&c<=90) cout<<"UPPER";
  else if(c>=97&&c<=122) cout<<"LOWER";
  else if(c>=48&&c<=57) cout<<"DIGIT";
  else cout<<"SPECIAL";
