  double a,b,c,d; cin>>a>>b>>c>>d;
  double dtb=(a+b+c*2+d*3)/7;
  if(dtb>=8) cout<<"gioi";
  else if(dtb<8&&dtb>=6.5) cout<<"kha";
  else if(dtb<6.5&&dtb>=5) cout<<"trung binh";
  else cout<<"yeu";
