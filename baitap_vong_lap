#18
  ll n; cin>>n;
  int dem=0;
  while(n){
    int r=n%10;
    if(r==2||r==3||r==5||r==7) dem++;
    n/=10;
  }
  cout<<dem;
#19
  int n; cin>>n;
  int vochai,bia;
  bia=n/28;
  vochai=bia;
  while(vochai>=3){
    int bia_moi=vochai/3;
    bia+=bia_moi;
    vochai=bia_moi+vochai%3;
  }
  cout<<bia;
#20 
  int n; cin>>n;
  if(n<2) {
    cout<<-1;
    return 0;
  }
  else if(n%2==0){
    cout<<n/2<<endl;
    for(int i=1;i<=n/2;i++){
      cout<<2<<" ";
    }
  }
  else {
    cout<<n/2<<endl;
    for(int i=1;i<=n/2-1;i++) cout<<2<<" ";
    cout<<3;
  }
#21 
  int n; cin>>n;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++) cout<<"*";
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(i==1||j==1||j==n||i==n) cout<<"*";
      else cout<<" ";
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(i==1||j==1||j==n||i==n) cout<<"*";
      else cout<<"#";
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(i==1||i==n||j==1||j==n) cout<<i<<" ";
      else cout<<"  ";
    }
    cout<<endl;
  }
#22
  int n; cin>>n;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=i;j++) cout<<"*";
    cout<<endl;
  }
  cout<<endl;
  for(int i=n;i>=1;i--){
    for(int j=1;j<=i;j++) cout<<"*";
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(n-i>=j) cout<<" ";
      else cout<<"*";
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=n;i>=1;i--){
    for(int j=1;j<=n;j++){
      if(n-i>=j) cout<<" ";
      else cout<<"*";
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(j==1||i==n||j==i) cout<<"*";
      else cout<<" ";
    }
    cout<<endl;
  }
#23
  int n; cin>>n;
  int dem=1;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      cout<<dem<<" ";
      dem++;
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    int dem=i;
    for(int j=1;j<=n;j++){
      cout<<dem<<" ";
      dem++;
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
      if(n-i>=j) cout<<"~";
      else cout<<i;
    }
    cout<<endl;
  }
  cout<<endl;
  for(int i=1;i<=n;i++){
    int step=n-1,buoc=i;
    for(int j=1;j<=i;j++){
      cout<<buoc<<" ";
      buoc+=step;
      step--;
    }
    cout<<endl;
  }
#24
  int a,b; cin>>a>>b;
  int min_val=min(a,b); // tìm số nhỏ rồi dùng for tính gt 
  int gt=1;
  for(int i=1;i<=min_val;i++) gt*=i;
  cout<<gt;
#25
  int n; cin>>n;
  double tong=1;
  int gt=1;
  for(int i=1;i<n;i++){
    gt*=i;
    tong+=1.0/gt;
  }
  cout<<fixed<<setprecision(4)<<tong;
#26
  int a,b,n; cin>>a>>b>>n;
  int ok=0;
  for(int x=0;x<=n/a;x++){
    int y=(n-a*x);
    if(y%b==0&&y>=0) ok=1;
  }
  if(ok) cout<<"yes";
  else cout<<"no";
#27
  int n; cin>>n;
  while(n>9){
    int sum=0;
    while(n){
      sum+=n%10;
      n/=10;
    }
    n=sum;
  }
  cout<<n;
