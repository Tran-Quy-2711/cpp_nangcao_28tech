  int tong=0;
  for(int i=0;i<=n;i+=3) tong+=i;
  cout<<tong;
#4
  int n; cin>>n;
  double tong=0;
  for(int i=1;i<=n;i++) tong+=1.0/i;
  cout<<fixed<<setprecision(3)<<tong;
#5
  int n; cin>>n;
  double tong=0;
  for(int i=1;i<=n;i++) tong+=1.0/(2*i);
  cout<<fixed<<setprecision(5)<<tong;
#6
  int n; cin>>n;
  int tong=0;
  for(int i=1;i*i<=n;i++){
    if(n%i==0){
      tong+=i;
      if(i!=n/i) tong+=n/i; // tránh cộng trùng căn bậc hai
    }
  }
  cout<<tong;
#7
  int n; cin>>n;
  int dem=0;
  for(int i=1;i*i<=n;i++){
    if(n%i==0){
      dem++;
      if(i!=n/i) dem++;
    }
  }
  cout<<dem<<endl;
  for(int i=1;i<=n;i++){  // liệt kê tăng dần thì không tối ưu 
    if(n%i==0){
      cout<<i<<" ";
    }
  }
#8
  int n; cin>>n;
  for(int i=1;i*i<=n;i++){
    cout<<i*i<<" ";
  }
#9
  int n; cin>>n;
  int tich=1;
  for(int i=1;i*i<=n;i++){
    if(n%i==0){
      tich*=i;
      if(i!=n/i) tich*=n/i;
    }
  }
  cout<<tich;
#10
  int n; cin>>n;
  int x; 
  int ok=0;
  for(int i=1;i<=n;i++){
    cin>>x;
    if(x==2022) ok=1;
  }
  if(ok) cout<<"yes";
  else cout<<"no";
