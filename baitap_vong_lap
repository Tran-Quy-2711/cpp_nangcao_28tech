for (khởi_tạo; điều_kiện_lặp; cập_nhật) {
    // khối lệnh thực hiện trong mỗi vòng lặp
}
NOTE : 
  - Gặp ước thì tối ưu : i*i<=n 
  - số chính phương không phải là ước không dùng n%i==0 -> cout<<i*i; vẫn tối ưu 
[Vòng lặp Cơ Bản]
#1 
  int n; cin>>n;
  for(int i=1;i<=n;i++) cout<<"Hello 28tech"<<endl;
  for(int i=1;i<=n;i++) cout<<i<<" ";
  cout<<endl;
  for(int i=0;i<n;i++) cout<<i<<" ";
  cout<<endl;
  for(int i=n;i>=1;i--) cout<<i<<" ";
  cout<<endl;
  for(int i=n-1;i>=0;i--) cout<<i<<" ";
#2 
  int n; cin>>n;
  for(int i=0;i<=n;i+=3) cout<<i<<" ";
  cout<<endl;
  for(int i=1;i<=n;i*=2){
    cout<<i<<" "; // in ra cout trước rồi dùng giá trị cập nhật 
  }
  cout<<endl;
  for(int i=n;;i++){
    if(i%17==0) {
      cout<<i;
      break;
    }
  }
  cout<<endl;
  for(int i=n;;i--){
    if(i%7==0){
      cout<<i;
      break;
    }
  }
  cout<<endl;
  int dem=1,step=1;
  for(int i=1;dem<=n;i++){
    cout<<dem<<" ";
    dem+=step;
    step++;
  }
#3 
DÙNG FOR 
  int n; cin>>n;
  for(int i=1;i<=n;i++) cout<<i<<" ";
  cout<<endl;
  for(int i=n;i>=0;i--) cout<<i<<" ";
  cout<<endl;
  for(int i=0;i<=n;i+=2) cout<<i<<" ";
  cout<<endl;
  for(int i=1;i<=n;i+=2) cout<<i<<" ";
  cout<<endl;
  for(int i=0;i<=n;i+=4) cout<<i<<" ";
  cout<<endl;
  for(int i=97;i<=96+n;i++) cout<<char(i)<<" ";
  cout<<endl;
  for(char i='v';i<='v'-1+n;i++) cout<<char(i)<<" ";
DÙNG WHILE 
  int n; cin>>n;
  int i=1;
  while(i<=n){
    cout<<i<<" ";
    i++;
  }
  cout<<endl;
  int i2=n;
  while(i2>=0){
    cout<<i2<<" ";
    i2--;
  }
  cout<<endl;
  int i3=0;
  while(i3<=n){
    cout<<i3<<" ";
    i3+=2;
  }
  cout<<endl;
  int i4=1;
  while(i4<=n){
    cout<<i4<<" ";
    i4+=2;
  }
  cout<<endl;
  int i5=0;
  while(i5<=n){
    cout<<i5<<" ";
    i5+=4;
  }
  cout<<endl;
  int i6=97;
  while(i6<=96+n){
    cout<<char(i6)<<" ";
    i6++;
  }
  cout<<endl;
  char i7='v';
  while(i7<='v'-1+n){
    cout<<char(i7)<<" ";
    i7++;
  }
#4 
  int n; cin>>n;
  int i=3;
  while(i<=n){
    cout<<i<<" ";
    i+=3;
  }
  cout<<endl;
  int i2=0;
  while(i2<=n){
    cout<<i2<<" ";
    i2+=15;
  }
  cout<<endl;
  int i3=n+1;
  while(i3>=n){
    if(i3%7==0){
      cout<<i3;
      break;
    }
    i3++;
  }
  cout<<endl;
  int i4=n-1;
  while(i4<=n){
    if(i4%9==0){
      cout<<i4;
      break;
    }
    i4--;
  }
  cout<<endl;
  int i5=1;
  while(i5<=2*n){
    cout<<i5<<" ";
    i5+=2;
  }
